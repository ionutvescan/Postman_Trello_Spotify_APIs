	"item": [
		{
			"name": "Get list users",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Response body is an object\", () => {\r",
							"    pm.response.to.be.an(\"object\");\r",
							"});\r",
							"\r",
							"const response = pm.response.json();\r",
							"\r",
							"pm.test(\"Data property is a list\", () => {\r",
							"    pm.expect(response.data).to.be.an(\"array\");\r",
							"});\r",
							"\r",
							"pm.test(\"Page number is 2\", () => {\r",
							"    pm.expect(response.page).to.eql(2);\r",
							"});\r",
							"\r",
							"pm.test(\"Author Tobias Funke exists in list\", () => {\r",
							"    let authorName = response.data.find(authorName => authorName.first_name === \"Tobias\");\r",
							"    console.log(authorName);\r",
							"    pm.expect(authorName.first_name).to.eql(\"Tobias\");\r",
							"    pm.expect(authorName.last_name).to.eql(\"Funke\");\r",
							"});\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{baseURL}}/api/users?page=2",
					"host": [
						"{{baseURL}}"
					],
					"path": [
						"api",
